% array array => array
/CreateEXPREnv
{
  4 dict begin
    /tmp_aarg exch def
    dup
    /tmp_farg exch CDR CAR def
    /tmp_fenv exch CAR def
    /tmp_ret CreateNewEnv def
    {
      tmp_farg nil eq
      {
        exit
      }if
      tmp_ret tmp_farg CAR tmp_aarg CAR Pair AddToEnv
      /tmp_aarg tmp_aarg CDR def
      /tmp_farg tmp_farg CDR def
    }loop
    tmp_ret tmp_fenv Nconc
    tmp_ret
  end
}def

